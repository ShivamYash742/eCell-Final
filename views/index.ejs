        <div class="social-feed-section">
            <h2 class="head">Social Feed</h2>
            <div class="container" id="postContainer">
                <% posts.slice(0, 3).forEach(post => { %>
                    <div class="post">
                        <div class="image-container">
                            <img src="<%= post.local_image_url %>" 
                                alt="Instagram Post"
                                onerror="this.src='/images/placeholder.jpg'">
                        </div>
                        <div class="content">
                            <p class="caption"><%= post.caption %></p>
                            <a href="<%= post.post_url %>" target="_blank" rel="noopener noreferrer" class="post-link">
                                View on Instagram →
                            </a>
                        </div>
                    </div>
                <% }) %>
            </div>
            <div class="load-more" style="margin-left: auto; margin-right: auto; display: block; width: fit-content;">
                <button onclick="loadMore()">Load More</button>
            </div>
        </div>

        <script>
            let currentIndex = 3;
            const posts = JSON.parse('<%- JSON.stringify(posts) %>');
            const postContainer = document.getElementById('postContainer');

            function loadMore() {
                const remainingPosts = posts.slice(currentIndex, currentIndex + 2); // Load one card at a time
                remainingPosts.forEach(post => {
                    const postElement = document.createElement('div');
                    postElement.classList.add('post');
                    postElement.innerHTML = `
                        <div class="image-container">
                            <img src="${post.local_image_url}" 
                                alt="Instagram Post" 
                                onerror="this.src='/images/placeholder.jpg'">
                        </div>
                        <div class="content">
                            <p class="caption">${post.caption}</p>
                            <a href="${post.post_url}" target="_blank" rel="noopener noreferrer" class="post-link">
                                View on Instagram →
                            </a>
                        </div>
                    `;
                    postContainer.appendChild(postElement);
                });

                currentIndex += 2;
                if (currentIndex >= posts.length) {
                    const loadMoreButton = document.querySelector('.load-more button');
                    if (loadMoreButton) {
                        loadMoreButton.style.display = 'none'; // Hide the button
                    }
                }
            }
        </script>
